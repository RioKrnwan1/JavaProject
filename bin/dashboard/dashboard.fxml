<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="600" prefWidth="800" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dashboard.DashboardController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="266.0" prefWidth="600.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Sistem Informasi Pelayanan">
               <font>
                  <Font size="24.0" />
               </font>
            </Text>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Selamat Datang">
               <font>
                  <Font size="18.0" />
               </font>
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </Text>
            <HBox alignment="CENTER" prefHeight="44.0" prefWidth="600.0" spacing="30.0">
               <children>
                  <Button fx:id="btnLogout" mnemonicParsing="false" onAction="#btnLogout" prefHeight="30.0" prefWidth="86.0" style="-fx-background-color: #4DA6FF; -fx-background-radius: 10px;" text="Logout" textFill="WHITE">
                     <font>
                        <Font name="Arial Bold" size="14.0" />
                     </font>
                  </Button>
                  <Button fx:id="btnUser" mnemonicParsing="false" onAction="#btnUser" prefHeight="30.0" prefWidth="90.0" style="-fx-background-color: #4DA6FF; -fx-background-radius: 10px;" text="Data User" textFill="WHITE">
                     <font>
                        <Font name="Arial Bold" size="14.0" />
                     </font>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </HBox>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="\@copyright 2025">
               <font>
                  <Font size="10.0" />
               </font>
               <VBox.margin>
                  <Insets top="40.0" />
               </VBox.margin>
            </Text>
         </children>
      </VBox>
   </children>
</VBox>
